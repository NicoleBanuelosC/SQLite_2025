<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_consultas"
        android:layout_width="150dp"
        android:layout_height="104dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="131dp"
        android:text="@string/txt_btn_consultas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_cambios" />

    <Button
        android:id="@+id/btn_cambios"
        android:layout_width="150dp"
        android:layout_height="104dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="131dp"
        android:text="@string/txt_btn_cambios"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_bajas" />

    <Button
        android:id="@+id/btn_bajas"
        android:layout_width="150dp"
        android:layout_height="104dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="22dp"
        android:layout_marginBottom="20dp"
        android:text="@string/txt_btn_bajas"
        app:layout_constraintBottom_toTopOf="@+id/btn_cambios"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_altas" />

    <Button
        android:id="@+id/btn_altas"
        android:layout_width="150dp"
        android:layout_height="104dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="131dp"
        android:text="@string/txt_btn_altas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>